<UserControl x:Class="ModengTerm.Addons.ResourceMgr.ResourceManagerUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ModengTerm.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <local:SessionTreeViewUserControl x:Name="SessionTreeViewUserControl" DataContext="{Binding Path=ResourceManagerTree}"
                                          TreeViewMouseDoubleClick="SessionTreeViewUserControl_TreeViewMouseDoubleClick"/>
        
    </Grid>
</UserControl>
