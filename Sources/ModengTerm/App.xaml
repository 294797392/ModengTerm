<Application x:Class="XTerminal.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" Startup="Application_Startup">

    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ModengTerm;component/Themes/Chinese.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ModengTerm;component/Themes/Themes.Default.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ModengTerm;component/Styles/SVGIcons.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ModengTerm;component/Styles/Generic.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ModengTerm.Document.Rendering;component/Template.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>

</Application>
